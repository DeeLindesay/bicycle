{
  "presets": ["forbeslindesay"],
  "plugins": [
    "transform-flow-comments",
    [
      "module-alias",
      [
        {
          "src": "./src",
          "expose": "bicycle"
        }
      ]
    ]
  ],
  "env": {
    "development": {
      "plugins": [["flow-runtime", {
        "assert": true,
        "decorate": true
      }]]
    },
    "production": {
      "plugins": []
    }
  }
}
