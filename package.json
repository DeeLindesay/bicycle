{
  "name": "bicycle",
  "version": "0.0.15",
  "description": "A data synchronisation library for JavaScript",
  "keywords": [],
  "dependencies": {
    "body-parser": "^1.14.2",
    "character-parser": "^2.1.1",
    "express-graphql": "^0.4.5",
    "graphql": "^0.4.14",
    "promise": "^7.0.4",
    "stable-sha1": "^1.0.1",
    "then-request": "^2.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-eslint": "^4.1.6",
    "babel-plugin-syntax-trailing-function-commas": "^6.3.13",
    "babel-plugin-transform-class-properties": "^6.3.13",
    "babel-plugin-transform-export-extensions": "^6.3.13",
    "babel-plugin-transform-flow-comments": "^6.3.23",
    "babel-plugin-transform-flow-strip-types": "^6.3.15",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-plugin-transform-react-display-name": "^6.3.13",
    "babel-plugin-transform-react-jsx": "^6.3.13",
    "babel-plugin-transform-strict-mode": "^6.3.13",
    "babel-plugin-typecheck": "^3.5.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify-middleware": "^7.0.0",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.13.0",
    "express": "^4.13.3",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "todomvc-app-css": "^2.0.3",
    "todomvc-common": "^1.0.2"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run build:prod && npm run build:dev",
    "build:prod": "NODE_ENV=production babel src --out-dir lib",
    "build:dev": "babel src --out-dir dev",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bicyclejs/bicycle.git"
  },
  "author": "ForbesLindesay",
  "license": "MIT",
  "developmentVersions": {
    "bicycle/lib/": "bicycle/dev/"
  },
  "babel": {
    "plugins": [
      "syntax-trailing-function-commas",
      "transform-class-properties",
      "transform-export-extensions",
      "transform-object-rest-spread",
      "transform-react-display-name",
      "transform-react-display-name",
      "transform-react-jsx",
      "transform-strict-mode",
      "transform-flow-comments"
    ],
    "env": {
      "development": {
        "plugins": [
          "typecheck"
        ]
      }
    },
    "presets": [
      "es2015"
    ]
  }
}